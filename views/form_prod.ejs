    <!--ventas/diego -->
    <section class="ventas">   
        <div class="categorias-title mt-3">
          <h2 class="title_categoria border-bottom">Vende en CanillaLibre.com</h2>
        </div>
        
        <div class="container bg-light mb-5"> 
            
            <div>
                <div class="form-group">
                    <h3>¿Que es lo que estas vendiendo?</h3>
                    <label for="producto">Titulo de anuncio*</label>
                    <input
                    type="text"
                    class="form-control"
                    id="title_prod"
                    placeholder="Introduce el titulo de tu producto"
                    required/>
                </div>
                <div>
                    <label for="contact">Numero de Contacto</label>
                    <input
                    type="text"
                    class="form-control"
                    id="contact"
                    placeholder="Introduce tu numero de telefono"
                    />
                </div>
            </div>
            <div class="descripcion form-group">
                <label for="descripcion_producto">Hace una breve despricion de lo que vas a vender</label>
                <div>
                    <textarea
                    id="descripcion_producto"
                    rows="4"
                    placeholder="No te olvides datos importantes"
                    class="col-12"
                    
                    ></textarea>
                </div>
                
            </div>
            <div class="form-group">
                <h4>Categoria Seleccionada</h4>
                <div>
                    <input type="radio" id="insumo" name="tipo_prod" value="insumo" checked />
                    <label for="insumo">Insumo</label>
                </div>
                <div>
                    <input type="radio" id="producto" name="tipo_prod" value="producto" />
                    <label for="producto">Producto</label>
                </div>
                <div>
                    <input type="radio" id="otros" name="tipo_prod" value="otros" />
                    <label for="otros">Otros/Cursos/Servicios<label>
                    </div>
                    
                </div>
                
                <div class="valor form-group">
                    <div>
                        <label for="precio">Precio*
                        </label>
                        <input
                        type="number"
                        class="form-control"
                        id="price"
                        placeholder="Precio"
                        required/>
                    </div>
                    
                    <div>
                        <label for="cantidad">Cantidad*</label>
                        <input
                        type="number"
                        class="form-control"
                        id="qty"
                        placeholder="Cantidad"
                        required>
                    </div>
                    
                    
                </div>
                <div class="adj_fotos">
                    <div>
                        <p>Como se ve?</p>
                        <div  class="fotos">
                            <label for="foto1">Adjuntar foto 1</label>
                            <input type="file" id="foto1" />
                        </div>
                        <div  class="fotos">
                            <label for="foto2">Adjuntar foto 2</label>
                            <input type="file" id="foto2" />
                        </div>
                        
                        <div  class="fotos">
                            <label for="foto3">Adjuntar foto 3</label>
                            <input type="file" id="foto3" />
                        </div>
                    </div>
                </div>
                <div>
                    <input type="checkbox" id="ofertas" name="ofertas" value="Aceptas Ofertas?" checked />
                    <label for="ofertas">Aceptas Ofertas?</label>
                </div>
                <div>
                    <input type="checkbox" id="envio" name="envio" value="Haces Envios" />
                    <label for="envios">Haces Envios?</label>
                </div>
                <div class="w-100 d-flex justify-content-center">
                  <button type="submit" class="btn btn-outline-warning d-flex">Guardar y Enviar</button>
                </div>
            </div>
            
        </div>
